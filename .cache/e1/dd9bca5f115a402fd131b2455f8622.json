root {\n//     --bg-color: black;\n//     --sub-color: #2B2E32;\n//     --red-color: #F77F7F;\n//     --green-color: #47FFE8;\n// }\n\n$bg-color: #1f1f1f;// #2B2E32;\n$sub-color: #2f3642;\n$grey-color-01: #454545;\n$red-color: #F77F7F;\n$green-color: #47FFE8;\n$yellow-color: yellow;\n$white-color: white;\n\n/* Global CSS Setup */\nbody {\n    // font-family: '04b03', serif;\n    font-family: \"Helvetica Neue\",Helvetica,\"Noto Sans\",sans-serif;\n    /* font-family: 'Roboto Mono', monospace; */\n    /* font-family: 'Press Start 2P', cursive; */\n    color: white;\n}\n\nh1, h2, h3, h4 {\n    font-family: '04b03', serif;\n    // color: white;    \n}\n\na {\n    text-decoration: none;\n    color: inherit;\n    font-size: 16px;\n}\na:hover {\n    color: $red-color;\n}\n\nh1 {\n    font-size: 4em;\n}\n\np {\n    font-size: 14px;\n}\n\n\n","scss/_index.scss":"body {\n    margin: auto;\n}\n\n.main-container {\n    display: grid;\n    grid-template-columns: auto 1fr;\n    min-height: 100vh;\n}\n\n/* Game Navigation */\n.nav-container {\n    background-color: $bg-color;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    padding: 0px 50px 0px 50px;\n\n    .title {\n        h1 {\n            margin-bottom: 0px;\n        }\n    }\n    hr {\n        width: 10px;\n    }\n\n    footer {\n        font-family: $font-04b03;\n        margin-top: 50px;\n        a {\n            font-size: 14px;\n\n            &:hover {\n                text-decoration: underline;\n            }\n        }\n    }\n}\n\n.game-highscore {\n    background-color: white;\n    color: $bg-color;\n    padding: 10px;\n\n    display: flex;\n    align-items: center;\n    flex-direction: column;\n}\n\n/* Game Selection */\n.game-container {\n    background-color: $sub-color;\n    display: flex;\n    flex-wrap: wrap;\n}\n.game {\n    margin: 50px;\n    height: 250px;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n\n    /* border: white solid 1px;  */\n}\n\n.game-thumbnail {\n\n    border: $white-color 10px solid;\n    box-shadow: 0px 0px 40px 7px $bg-color;\n\n    height: 200px;\n\n    &:hover {\n        border: $yellow-color 10px solid;\n        transform: translateY(-2px);\n    }\n}\n","scss/_highscore.scss":".game-highscore {\n    background-color: $grey-color-01;\n    color: $white-color;\n\n    font-family: $font-04b03;\n    font-size: 14px;\n}","scss/_click.scss":"body {\n    background-color: $sub-color;\n    margin: 0;\n}\n.click-container {\n    display: grid;\n    grid-template-columns: auto 1fr;\n    min-height: 100vh;\n\n    .click-highscore {\n        margin-left: 20px;\n        font-family: $font-04b03;\n        font-size: 16px;\n\n        .return-back {\n            display: flex;\n            align-items: center;\n\n            p {\n                margin-left: 10px;\n            }\n        }\n    }\n\n    .game {\n        margin-top: 300px;\n        display: flex;\n        flex-direction: column;\n        align-items: center;\n        justify-content: center;\n        /* border: white solid 1px; */\n    }\n}\n\n#click-button {\n    font-family: '04b03', serif;\n    font-size: 5em;\n    color: black;\n    background-color: white;\n\n    // height: 30vh;\n    padding: 50px;\n    width: 30vw;\n    border: $red-color 10px solid;\n\n    &:hover {\n        color: white;\n        background: $red-color;\n    }\n\n    &:active {\n        transform: translateY(4px);\n        box-shadow: 0 5px #666;\n    }\n}\n\n#score {\n    font-size: 10em;\n    font-family: $font-04b03;\n}\n\n/* Submit User Score */\n#playerName {\n    background-color: #40464c;\n    height: auto;\n    width: 35vw;\n    border: none;\n    font-family: '04b03', serif;\n    font-size: 5em;\n    color: white;\n}\n\n#highScoreUpdateButton {\n    font-family: '04b03', serif;\n    font-size: 2em;\n    color: black;\n    background-color: white;\n\n    padding: 20px;\n    width: 20vw;\n    border: $green-color 10px solid;\n\n    &:hover {\n        color: white;\n        background: $green-color;\n    }\n\n    &:active {\n        transform: translateY(4px);\n        box-shadow: 0 5px #666;\n    }\n}"},"lineCount":null}},"error":null,"hash":"81ac5c5692a2b7533bb580b8422af858","cacheData":{"env":{}}}