ts.googleapis.com/css?family=Press+Start+2P&display=swap');\n@import url('https://fonts.googleapis.com/css?family=Roboto+Mono&display=swap'); */\n@font-face {\n    font-family: '04b03';\n    src: url('assets/fonts/04B03.ttf');\n    font-weight: normal;\n    font-style: normal;\n}\n\n\n/* Color Setup */\n:root {\n    --bg-color: black;\n    --sub-color: #2B2E32;\n    --red-color: #F77F7F;\n    --green-color: #47FFE8;\n}\n\n/* Global CSS Setup */\nbody {\n    font-family: '04b03', serif;\n    /* font-family: 'Roboto Mono', monospace; */\n    /* font-family: 'Press Start 2P', cursive; */\n    color: white;\n}\n\na {\n    text-decoration: none;\n    color: inherit;\n}\na:hover {\n    color: var(--red-color);\n}\n\nh1 {\n    font-size: 4em;\n}\n\n\n","scss/_index.scss":"body {\n    margin: auto;\n}\n\n.main-container {\n    display: grid;\n    grid-template-columns: auto 1fr;\n    min-height: 100vh;\n}\n\n/* Game Navigation */\n.nav-container {\n    background-color: var(--bg-color);\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    padding: 0px 50px 0px 50px;\n\n    .title {\n        h1 {\n            margin-bottom: 0px;\n        }\n    }\n    hr {\n        width: 10px;\n    }\n}\n\n.game-highscore {\n    background-color: white;\n    color: var(--bg-color);\n    padding: 10px;\n\n    display: flex;\n    align-items: center;\n    flex-direction: column;\n}\n\n/* Game Selection */\n.game-container {\n    background-color: var(--sub-color);\n    display: flex;\n    flex-wrap: wrap;\n}\n.game {\n    margin: 50px;\n    height: 250px;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n\n    /* border: white solid 1px;  */\n}\n\n.game-thumbnail {\n    // -webkit-transition: all 1s cubic-bezier(0.165, 0.84, 0.44, 1);\n    // transition: all 1s cubic-bezier(0.165, 0.84, 0.44, 1);\n\n    border: var(--red-color) 10px solid;\n    box-shadow: 0px 0px 40px 7px var(--bg-color);\n}\n.game-thumbnail::after {\n    // -webkit-transition: all 1s cubic-bezier(0.165, 0.84, 0.44, 1);\n    // transition: all 1s cubic-bezier(0.165, 0.84, 0.44, 1);\n}\n.game-thumbnail:hover {\n    border: var(--green-color) 10px solid;\n    transform: translateY(-4px);\n    // transform: scale(1.05, 1.05);\n}","scss/_click.scss":"body {\n    background-color: var(--sub-color);\n    margin: 0;\n}\n.click-container {\n    display: grid;\n    grid-template-columns: auto 1fr;\n    min-height: 100vh;\n\n    .click-highscore {\n        margin-left: 20px;\n\n        .return-back {\n            display: flex;\n            align-items: center;\n\n            p {\n                margin-left: 10px;\n            }\n        }\n    }\n\n    .game {\n        margin-top: 300px;\n        display: flex;\n        flex-direction: column;\n        align-items: center;\n        justify-content: center;\n        /* border: white solid 1px; */\n    }\n}\n\n#click-button {\n    font-family: '04b03', serif;\n    font-size: 5em;\n    color: black;\n    background-color: white;\n\n    // height: 30vh;\n    padding: 50px;\n    width: 30vw;\n    border: var(--red-color) 10px solid;\n\n    &:hover {\n        color: white;\n        background: var(--red-color);\n    }\n\n    &:active {\n        transform: translateY(4px);\n        box-shadow: 0 5px #666;\n    }\n}\n\n#score {\n    font-size: 10em;\n}\n\n/* Submit User Score */\n#playerName {\n    background-color: #40464c;\n    height: auto;\n    width: 35vw;\n    border: none;\n    font-family: '04b03', serif;\n    font-size: 5em;\n    color: white;\n}\n\n#highScoreUpdateButton {\n    font-family: '04b03', serif;\n    font-size: 2em;\n    color: black;\n    background-color: white;\n\n    padding: 20px;\n    width: 20vw;\n    border: var(--green-color) 10px solid;\n\n    &:hover {\n        color: white;\n        background: var(--green-color);\n    }\n\n    &:active {\n        transform: translateY(4px);\n        box-shadow: 0 5px #666;\n    }\n}"},"lineCount":null}},"error":null,"hash":"fc253be279cc86bda63d0c89dccf1fcf","cacheData":{"env":{}}}